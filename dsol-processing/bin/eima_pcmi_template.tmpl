<mindex>
#for $page in $pages
  <comp type="IP">
    <name>$page['pagenum']</name>
    <order>$page['pageseq']</order>
    <rep type="THUM">
      <path>/media/ch/$page['product']/pc/$page['pcid']/page/$page['pageseq']/img.jpg</path>
    </rep>
    <rep type="IMP">
      <path>/media/ch/$page['product']/pc/$page['pcid']/page/$page['pageseq']/pagemap.xml</path>
    </rep>
  </comp>
#end for
</mindex>
  
