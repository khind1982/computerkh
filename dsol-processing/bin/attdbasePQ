#!/usr/local/bin/python2.6
# -*- mode: python -*-

import sys, os
sys.path.append(os.path.join(os.path.dirname(__file__) + '/../lib/python'))

from abell import processAbell

if len(sys.argv) == 1:
    filename = "abell.lau"
    sys.stderr.write("Using default file name 'abell.lau'\n")
elif len(sys.argv) == 2:
    filename = sys.argv[1]

processAbell.attdbasePQ([filename])
