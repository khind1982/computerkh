<!-- This is the Global Schema v5.0 which defines the format of the content stored for the Polaris release of the Morningstar project -->
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
	<xs:include schemaLocation="References_v8.xsd"/>
	<!-- <xs:include schemaLocation="https://schemas.proquest.com/referencelinking/References_v8.xsd"/> -->
	<xs:element name="IngestRecord">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="MinorVersion">
					<xs:simpleType>
						<xs:restriction base="xs:integer">
							<xs:minInclusive value="1"/>
							<xs:maxInclusive value="77"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="ControlStructure">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="ActionCode">
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:enumeration value="add"/>
										<xs:enumeration value="change"/>
										<xs:enumeration value="delete"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element ref="LegacyPlatform"/>
							<xs:element ref="LegacyID"/>
							<xs:element name="LastLegacyUpdateTime">
								<xs:simpleType>
									<xs:restriction base="xs:unsignedLong">
										<xs:totalDigits value="14"/>
										<xs:minInclusive value="00000000000001"/>
										<xs:maxInclusive value="99999999999999"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="IngestReason" minOccurs="0">
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:enumeration value="RELOAD"/>
										<xs:enumeration value="EDITORIAL"/>
										<xs:enumeration value="REMANUFACTURE"/>
										<xs:enumeration value="INT_REMAN"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="Cover" type="xs:boolean" minOccurs="0"/>
							<xs:element name="RelatedLegacyID" minOccurs="0" maxOccurs="unbounded">
								<xs:complexType>
									<xs:sequence>
										<xs:element ref="LegacyPlatform"/>
										<xs:element ref="LegacyID"/>
										<xs:element name="RelationshipType">
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:enumeration value="DerivedFrom"/>
													<xs:enumeration value="SeeAlso"/>
													<xs:enumeration value="Parent"/>
													<xs:enumeration value="Child"/>
													<xs:enumeration value="SameAs"/>
													<xs:enumeration value="TopicPageLink"/>
													<xs:enumeration value="RTLink"/>
													<xs:enumeration value="SIRSLink"/>
													<xs:enumeration value="EmbasePUI"/>
													<xs:enumeration value="RelatedVideo"/>
													<xs:enumeration value="RelatedAuthor"/>
													<xs:enumeration value="RelatedLitWork"/>
													<xs:enumeration value="RelatedPsychExperiment"/>
													<xs:enumeration value="RelatedBook"/>
													<xs:enumeration value="RelatedTopicPage"/>
													<xs:enumeration value="RelatedFeaturedTopicPage"/>
													<xs:enumeration value="RelatedImageSet"/>
													<xs:enumeration value="RelatedIllustration"/>
													<xs:enumeration value="RelatedArticle"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="SeeAlsoRelationship" minOccurs="0" maxOccurs="unbounded">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="SeeAlsoID" type="xs:string"/>
										<xs:element name="SeeAlsoIDType">
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:enumeration value="Accnum"/>
													<xs:enumeration value="GISSTAT"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="PublicationInfo" minOccurs="0">
								<xs:complexType>
									<xs:choice>
										<xs:element name="LegacyPubID" type="xs:string" minOccurs="0"/>
										<xs:element name="PubLocators" minOccurs="0">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="PubTitle" type="xs:string"/>
													<xs:element ref="Locator" maxOccurs="unbounded"/>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
									</xs:choice>
								</xs:complexType>
							</xs:element>
							<xs:element ref="Parent" minOccurs="0"/>
							<xs:element name="IndexedImageOBR" type="xs:boolean" minOccurs="0"/>
							<xs:element name="IndexedImageOBJ" type="xs:boolean" minOccurs="0"/>
							<xs:element name="RefLinkingData" minOccurs="0">
								<xs:complexType>
									<xs:choice>
										<xs:element name="Reference" type="xs:string" maxOccurs="unbounded"/>
										<xs:element ref="ItemWithRef_v2" minOccurs="0"/>
										<xs:element name="DeleteExisting">
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:enumeration value="Y"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
									</xs:choice>
								</xs:complexType>
							</xs:element>
							<xs:element name="ObjectBundleData" minOccurs="0" maxOccurs="unbounded">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="ObjectBundleType">
											<xs:annotation>
												<xs:documentation>ObjectBundleType corresponds to the values in Oracle table VALID_CNT_ITEM_BUNDLE_CODES.  However, there are a few that are mapped for historical reasons.  The following values are mapped both on Inbound check-in and the mapping is reversed for OODS pull as Ingest.  CSADBCode -> DBCODE, DissSubjectCode -> SUBJECT_CODE, DissSchoolCode -> SCHOOL_CODE</xs:documentation>
											</xs:annotation>
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:enumeration value="CSADBCode"/>
													<xs:enumeration value="CHProductCode"/>
													<xs:enumeration value="PQProductCode"/>
													<xs:enumeration value="DissSchoolCode"/>
													<xs:enumeration value="DissSubjectCode"/>
													<xs:enumeration value="DSMedlineStatusCode"/>
													<xs:enumeration value="DSDrugStatusCode"/>
													<xs:enumeration value="SIRSProductCode"/>
													<xs:enumeration value="OfflineProductCode"/>
													<xs:enumeration value="ebraryBundleCode"/>
													<xs:enumeration value="DSEmbaseAlerts"/>
													<xs:enumeration value="DissProductCode"/>
													<xs:enumeration value="ASUnitAccessCode"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="ObjectBundleValue" type="xs:string"/>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="PublicAccessDetails" minOccurs="0">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="LicenseName" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
										<xs:element name="OAType" minOccurs="0" maxOccurs="unbounded">
											<xs:complexType>
												<xs:simpleContent>
													<xs:extension base="xs:string">
														<xs:attribute name="OASubType" type="xs:string" use="optional"/>
													</xs:extension>
												</xs:simpleContent>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="Rights" minOccurs="0">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="Right" maxOccurs="unbounded">
											<xs:complexType>
												<xs:simpleContent>
													<xs:extension base="NonEmptyString">
														<xs:attribute name="RightSource" type="NonEmptyString" use="required"/>
														<xs:attribute name="RightType" type="NonEmptyString" use="required"/>
													</xs:extension>
												</xs:simpleContent>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="Component" minOccurs="0" maxOccurs="unbounded">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="mstar_lm_media_id" type="xs:string" minOccurs="0"/>
										<xs:element name="InlineImageCount" type="xs:string" minOccurs="0"/>
										<xs:element name="PageCount" type="xs:string" minOccurs="0"/>
										<xs:element name="Name" type="xs:string" minOccurs="0"/>
										<xs:element name="CSAImageKey" type="xs:string" minOccurs="0"/>
										<xs:element name="OrderCategory" type="xs:string" minOccurs="0"/>
										<xs:element name="Order" type="xs:integer" minOccurs="0"/>
										<xs:element name="CHImageIDs" minOccurs="0">
											<xs:complexType>
												<xs:choice maxOccurs="unbounded">
													<xs:element name="CHID" type="xs:string" minOccurs="0"/>
													<xs:element name="PQID" type="xs:string" minOccurs="0"/>
												</xs:choice>
											</xs:complexType>
										</xs:element>
										<xs:element name="DSMediaLocatorID" type="xs:string" minOccurs="0"/>
										<xs:element name="LinkedComponentID" minOccurs="0">
											<xs:complexType>
												<xs:sequence>
													<xs:element ref="LegacyPlatform"/>
													<xs:element ref="LegacyID"/>
													<xs:element name="RelationshipType">
														<xs:simpleType>
															<xs:restriction base="xs:string">
																<xs:enumeration value="Child"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="Representation" minOccurs="0" maxOccurs="unbounded">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="MimeType" type="xs:string" minOccurs="0"/>
													<xs:element name="Resides" type="xs:string"/>
													<xs:element name="ImageType" minOccurs="0">
														<xs:simpleType>
															<xs:restriction base="xs:string">
																<xs:enumeration value="PDF"/>
																<xs:enumeration value="TIFF"/>
																<xs:enumeration value="JPEG"/>
																<xs:enumeration value="SPDF"/>
																<xs:enumeration value="TI"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
													<xs:element name="WordMapCoords" type="xs:boolean" minOccurs="0"/>
													<xs:element name="Color" minOccurs="0">
														<xs:simpleType>
															<xs:restriction base="xs:string">
																<xs:enumeration value="color"/>
																<xs:enumeration value="grayscale"/>
																<xs:enumeration value="BW"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
													<xs:element name="IllustrationInfo" type="xs:string" minOccurs="0"/>
													<xs:element name="LayoutInfo" type="xs:string" minOccurs="0"/>
													<xs:element name="Options" type="xs:string" minOccurs="0"/>
													<xs:element name="ImageHitHighlighting" type="xs:boolean" minOccurs="0"/>
													<xs:element name="Bytes" type="xs:integer" minOccurs="0"/>
													<xs:element name="PDFType" type="xs:string" minOccurs="0"/>
													<xs:element name="Misc" type="xs:string" minOccurs="0"/>
													<xs:element name="Seconds" type="xs:integer" minOccurs="0"/>
													<xs:element name="RepId" type="xs:string" minOccurs="0"/>
													<xs:element name="ResourceId" type="xs:string" minOccurs="0"/>
													<xs:element name="CitationCount" type="xs:string" minOccurs="0"/>
													<xs:element name="Scanned" type="xs:boolean" minOccurs="0"/>
													<xs:element name="LegacyFormat" minOccurs="0">
														<xs:simpleType>
															<xs:restriction base="xs:string">
																<xs:enumeration value="PageImage"/>
																<xs:enumeration value="ArticleImage"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
													<xs:element name="CHImageHitHighlighting" type="xs:boolean" minOccurs="0"/>
													<xs:element name="MediaKey" type="xs:string" minOccurs="0"/>
													<xs:element name="RepresentationLanguage" type="xs:string" minOccurs="0"/>
													<xs:element name="Link" type="xs:string" minOccurs="0"/>
													<xs:element name="IsPrimary" minOccurs="0">
														<xs:simpleType>
															<xs:restriction base="xs:string">
																<xs:enumeration value="Y"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
													<xs:element name="SuppObjectCount" type="xs:string" minOccurs="0"/>
													<xs:element name="VRVideo" type="NonEmptyString" minOccurs="0"/>
												</xs:sequence>
												<xs:attribute ref="RepresentationType" use="required"/>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
									<xs:attribute ref="ComponentType" use="required"/>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="RECORD" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Version">
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:enumeration value="Global_Schema_v5.1.xsd"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="MinorVersion" minOccurs="0">
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:enumeration value="1"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element ref="ObjectInfo"/>
							<xs:element ref="Attachments" minOccurs="0"/>
							<xs:element ref="Product" minOccurs="0"/>
							<xs:element name="PublicationInfo" minOccurs="0">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="PublicationTitle" type="PubTitle" minOccurs="0"/>
										<xs:element name="PubSubTitle" type="Title" minOccurs="0"/>
										<xs:element name="AbbrPubTitle" type="Title" minOccurs="0"/>
										<xs:element name="PubAltTitle" type="PubTitle" minOccurs="0" maxOccurs="unbounded"/>
										<xs:element name="MonographTitle" type="Title" minOccurs="0"/>
										<xs:element name="UnparsedPubData" type="xs:string" minOccurs="0"/>
										<xs:element name="Volume" type="xs:string" minOccurs="0"/>
										<xs:element name="Issue" type="xs:string" minOccurs="0"/>
										<xs:element name="Edition" type="xs:string" minOccurs="0"/>
										<xs:element name="NonStdCitInfo" type="xs:string" minOccurs="0"/>
										<xs:element name="Part" type="xs:string" minOccurs="0"/>
										<xs:element name="Supplement" type="xs:string" minOccurs="0"/>
										<xs:element name="Section" type="xs:string" minOccurs="0"/>
										<xs:element name="Series" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
										<xs:element name="PubCopyright" type="xs:string" minOccurs="0"/>
										<xs:element name="Locators" minOccurs="0">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="LocatorID" type="ObjectID" maxOccurs="unbounded"/>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="Publisher" minOccurs="0" maxOccurs="unbounded">
											<xs:complexType>
												<xs:choice maxOccurs="unbounded">
													<xs:element name="PersonName" type="xs:string" minOccurs="0"/>
													<xs:element name="PublisherCompName" type="xs:string" minOccurs="0"/>
													<xs:element name="OrganizationName" type="xs:string" minOccurs="0"/>
													<xs:element name="MailingAddress" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
													<xs:element name="EmailAddress" type="xs:string" minOccurs="0"/>
													<xs:element name="PhoneNumber" type="xs:string" minOccurs="0"/>
													<xs:element name="FaxNumber" type="xs:string" minOccurs="0"/>
													<xs:element name="CityName" type="xs:string" minOccurs="0"/>
													<xs:element name="StateName" type="xs:string" minOccurs="0"/>
													<xs:element name="PostCode" type="xs:string" minOccurs="0"/>
													<xs:element name="CountryName" type="xs:string" minOccurs="0"/>
													<xs:element name="OriginalForm" minOccurs="0">
														<xs:complexType>
															<xs:simpleContent>
																<xs:extension base="xs:string">
																	<xs:attribute name="Id" type="xs:integer" use="optional"/>
																	<xs:attribute name="IsNormalized" type="xs:boolean" use="optional"/>
																</xs:extension>
															</xs:simpleContent>
														</xs:complexType>
													</xs:element>
													<xs:element name="AltOriginalForm" minOccurs="0">
														<xs:complexType>
															<xs:simpleContent>
																<xs:extension base="xs:string">
																	<xs:attribute name="Id" type="xs:integer" use="optional"/>
																	<xs:attribute name="IsNormalized" type="xs:boolean" use="optional"/>
																</xs:extension>
															</xs:simpleContent>
														</xs:complexType>
													</xs:element>
													<xs:element name="PublisherURL" type="xs:string" minOccurs="0"/>
													<xs:element name="PublisherLocation" minOccurs="0">
														<xs:complexType>
															<xs:simpleContent>
																<xs:extension base="xs:string">
																	<xs:attribute name="Id" type="xs:integer" use="optional"/>
																	<xs:attribute name="IsNormalized" type="xs:boolean" use="optional"/>
																</xs:extension>
															</xs:simpleContent>
														</xs:complexType>
													</xs:element>
													<xs:element name="AltPublisherLocation" minOccurs="0">
														<xs:complexType>
															<xs:simpleContent>
																<xs:extension base="xs:string">
																	<xs:attribute name="Id" type="xs:integer" use="optional"/>
																	<xs:attribute name="IsNormalized" type="xs:boolean" use="optional"/>
																</xs:extension>
															</xs:simpleContent>
														</xs:complexType>
													</xs:element>
												</xs:choice>
												<xs:attribute name="ContactType" type="xs:string" use="optional"/>
											</xs:complexType>
										</xs:element>
										<xs:element name="URL" type="xs:string" minOccurs="0"/>
										<xs:element name="IssueTitle" type="xs:string" minOccurs="0"/>
										<xs:element name="CollectionTitle" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
										<xs:element name="BibNumber" type="xs:string" minOccurs="0"/>
										<xs:element name="Imprint" minOccurs="0">
											<xs:complexType>
												<xs:simpleContent>
													<xs:extension base="xs:string">
														<xs:attribute ref="HTMLContent" use="optional"/>
													</xs:extension>
												</xs:simpleContent>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element ref="LegacyData" minOccurs="0"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="ContribType">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:enumeration value="CoAuth"/>
				<xs:enumeration value="ProjDirector"/>
				<xs:enumeration value="ReviewAuth"/>
				<xs:enumeration value="RecordOwner"/>
				<xs:enumeration value="FOPContact"/>
				<xs:enumeration value="FOPPerson"/>
				<xs:enumeration value="Inventor"/>
				<xs:enumeration value="Advisor"/>
				<xs:enumeration value="CmteMember"/>
				<xs:enumeration value="Collaborator"/>
				<xs:enumeration value="Photography"/>
				<xs:enumeration value="Art"/>
				<xs:enumeration value="Costumes"/>
				<xs:enumeration value="Set"/>
				<xs:enumeration value="Music"/>
				<xs:enumeration value="Sound"/>
				<xs:enumeration value="SpEffects"/>
				<xs:enumeration value="Dance"/>
				<xs:enumeration value="MkupHair"/>
				<xs:enumeration value="Cast"/>
				<xs:enumeration value="RevItemAuth"/>
				<xs:enumeration value="StandIn"/>
				<xs:enumeration value="Animation"/>
				<xs:enumeration value="Color"/>
				<xs:enumeration value="SourceAuth"/>
				<xs:enumeration value="Author"/>
				<xs:enumeration value="CorpAuthor"/>
				<xs:enumeration value="Producer"/>
				<xs:enumeration value="Director"/>
				<xs:enumeration value="Speaker"/>
				<xs:enumeration value="Editor"/>
				<xs:enumeration value="Distributor"/>
				<xs:enumeration value="Publisher"/>
				<xs:enumeration value="Contact"/>
				<xs:enumeration value="Studio"/>
				<xs:enumeration value="CorrespAuthor"/>
				<xs:enumeration value="Other"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="LegacyID">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="255"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="LegacyPlatform">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:enumeration value="PQ"/>
				<xs:enumeration value="HNP"/>
				<xs:enumeration value="CSA"/>
				<xs:enumeration value="CH"/>
				<xs:enumeration value="DS"/>
				<xs:enumeration value="SIRS"/>
				<xs:enumeration value="COS"/>
				<xs:enumeration value="ELIB"/>
				<xs:enumeration value="MSTAR"/>
				<xs:enumeration value="GISSTAT"/>
				<xs:enumeration value="EBC"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="Locator">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="LocType">
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:enumeration value="ISSN"/>
							<xs:enumeration value="ElecISSN"/>
							<xs:enumeration value="ISBN"/>
							<xs:enumeration value="ISBN10"/>
							<xs:enumeration value="ISBN13"/>
							<xs:enumeration value="DOI"/>
							<xs:enumeration value="CODEN"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="LocValue" type="xs:string"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="Parent">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="ParentInfo">
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="LegacyPlatform" minOccurs="0"/>
							<xs:element ref="LegacyID" minOccurs="0"/>
							<xs:element name="Title" minOccurs="0">
